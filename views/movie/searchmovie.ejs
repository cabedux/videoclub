<!DOCTYPE html>
<html lang="en">
<head>
  <% include ../head.ejs %>
</head>
<body>
    <header>
        <% include ../navbar.ejs %>
    </header>
    <main>
        <div class="container pt-5">
            <div class="row">
              <div class="col-12 d-flex justify-content-between">
                <h1 class="">Resultado busqueda</h1>
              </div>
              <div class="col-12 grey">
                <table class="table table-hover">
                <thead>
                  <tr>
                    <th scope="col">#</th>
                    <th scope="col">Titulo</th>
                    <th scope="col">Descripci√≥n</th>
                    <th scope="col">Fecha</th>
                    <th scope="col">Genero</th>

                  </tr>
                </thead>
                <tbody>
                  <%dataSearch.forEach(function(movie){%>
                  <tr>
                    <th scope="row"><%= movie.movie_id%></th>
                    <td><%= movie.title%></td>
                    <td><%= movie.description%></td>
                    <td><%= movie.release_date%></td>
                    <td><%= movie.gender%></td>
                  </tr>
                  <%})%>
                </tbody>
                </table>
              </div>   
            </div>
          </div>
    </main>
    
</body>
</html>